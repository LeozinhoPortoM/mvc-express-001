<%- include("./partials/head") %>
<main>
  <h1>Usuários</h1>
  <% if(users.length > 0){ %>
    <% for (let usuario of users) { %>
      <div>
        <h2><%= usuario.nome %></h2>
        <span><%= usuario.sobrenome %></span>
        <p><%= usuario.idade %></p>
      </div>
    <% } %>
  <% }else {%>
    <h1>Nenhum usuário encontrado</h1>
  <% }%>

</main>
<%- include("./partials/footer") %>